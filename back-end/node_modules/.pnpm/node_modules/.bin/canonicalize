#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/user/Desktop/DataRand/back-end/node_modules/.pnpm/canonicalize@2.1.0/node_modules/canonicalize/bin/node_modules:/home/user/Desktop/DataRand/back-end/node_modules/.pnpm/canonicalize@2.1.0/node_modules/canonicalize/node_modules:/home/user/Desktop/DataRand/back-end/node_modules/.pnpm/canonicalize@2.1.0/node_modules:/home/user/Desktop/DataRand/back-end/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/user/Desktop/DataRand/back-end/node_modules/.pnpm/canonicalize@2.1.0/node_modules/canonicalize/bin/node_modules:/home/user/Desktop/DataRand/back-end/node_modules/.pnpm/canonicalize@2.1.0/node_modules/canonicalize/node_modules:/home/user/Desktop/DataRand/back-end/node_modules/.pnpm/canonicalize@2.1.0/node_modules:/home/user/Desktop/DataRand/back-end/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../canonicalize/bin/canonicalize.js" "$@"
else
  exec node  "$basedir/../canonicalize/bin/canonicalize.js" "$@"
fi
